{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "pdurgadatafactory"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
		},
		"AzureSqlDatabase1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase1'"
		},
		"AzureSqlDatabase2new_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSqlDatabase2new'"
		},
		"Final_target_AzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Final_target_AzureBlobStorage'"
		},
		"MySql_Onpremise_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'MySql_Onpremise'"
		},
		"AzureBlobStoragesouce_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'AzureBlobStoragesouce'"
		},
		"AzureBlobStoragetarget_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'AzureBlobStoragetarget'"
		},
		"AzureSqlDatabase1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "pdurgasqlserver.database.windows.net"
		},
		"AzureSqlDatabase1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "creativitydb"
		},
		"AzureSqlDatabase1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "creativity"
		},
		"AzureSqlDatabase2new_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "pdurgasqlserver.database.windows.net"
		},
		"AzureSqlDatabase2new_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "creativitydb"
		},
		"AzureSqlDatabase2new_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Access Token"
		},
		"Final_AzureBlobStorage_source_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'Final_AzureBlobStorage_source'"
		},
		"MySql_Onpremise_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "localhost"
		},
		"MySql_Onpremise_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().database_name}"
		},
		"MySql_Onpremise_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "root"
		},
		"blobsource_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'blobsource'"
		},
		"new blob_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'new blob'"
		},
		"newBlobStorage_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'newBlobStorage'"
		},
		"default_properties_schema_name_value": {
			"type": "string",
			"defaultValue": "dbo"
		},
		"default_properties_database_name_value": {
			"type": "string",
			"defaultValue": "familyinfodb"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "pdurgadatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDatabricks1_accessToken": {
						"value": "[parameters('AzureDatabricks1_accessToken')]"
					},
					"AzureSqlDatabase1_password": {
						"value": "[parameters('AzureSqlDatabase1_password')]"
					},
					"AzureSqlDatabase2new_password": {
						"value": "[parameters('AzureSqlDatabase2new_password')]"
					},
					"Final_target_AzureBlobStorage_connectionString": {
						"value": "[parameters('Final_target_AzureBlobStorage_connectionString')]"
					},
					"MySql_Onpremise_password": {
						"value": "[parameters('MySql_Onpremise_password')]"
					},
					"default_properties_schema_name_value": {
						"value": "[parameters('default_properties_schema_name_value')]"
					},
					"default_properties_database_name_value": {
						"value": "[parameters('default_properties_database_name_value')]"
					},
					"AzureBlobStoragesouce_sasUri": {
						"value": "[parameters('AzureBlobStoragesouce_sasUri')]"
					},
					"AzureBlobStoragetarget_sasUri": {
						"value": "[parameters('AzureBlobStoragetarget_sasUri')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase1_properties_typeProperties_userName')]"
					},
					"AzureSqlDatabase2new_properties_typeProperties_server": {
						"value": "[parameters('AzureSqlDatabase2new_properties_typeProperties_server')]"
					},
					"AzureSqlDatabase2new_properties_typeProperties_database": {
						"value": "[parameters('AzureSqlDatabase2new_properties_typeProperties_database')]"
					},
					"AzureSqlDatabase2new_properties_typeProperties_userName": {
						"value": "[parameters('AzureSqlDatabase2new_properties_typeProperties_userName')]"
					},
					"Final_AzureBlobStorage_source_sasUri": {
						"value": "[parameters('Final_AzureBlobStorage_source_sasUri')]"
					},
					"blobsource_sasUri": {
						"value": "[parameters('blobsource_sasUri')]"
					},
					"new blob_sasUri": {
						"value": "[parameters('new blob_sasUri')]"
					},
					"newBlobStorage_sasUri": {
						"value": "[parameters('newBlobStorage_sasUri')]"
					},
					"MySql_Onpremise_properties_typeProperties_server": {
						"value": "[parameters('MySql_Onpremise_properties_typeProperties_server')]"
					},
					"MySql_Onpremise_properties_typeProperties_database": {
						"value": "[parameters('MySql_Onpremise_properties_typeProperties_database')]"
					},
					"MySql_Onpremise_properties_typeProperties_username": {
						"value": "[parameters('MySql_Onpremise_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "pdurgadatafactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'pdurgadatafactory_ArmTemplate_0')]"
			]
		}
	]
}